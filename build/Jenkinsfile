imagename = "prabusampath007/mywebapp"
registryCredential = 'docker-credentials'

pipeline {
    agent { label 'ec2-agent-build'}
    stages {
        stage('Build image') {
            steps {
                dir('../webapp/Dockerfile') {
                    script{
                        dockerImage = docker.build(imagename)
                        docker.withRegistry( '', registryCredential ) {
                            dockerImage.push()
                        }
                    }
                }
            }
        }
    }
}