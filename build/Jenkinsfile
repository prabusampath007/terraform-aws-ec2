imagename = "prabusampath007/my-ubuntu"
registryCredential = 'docker-credentials'

pipeline {
    agent { label 'ec2-agent-build'}
    stages {
        stage('Build image') {
            steps {
                script{
                    dockerImage = docker.build(imagename)
                    docker.withRegistry( '', registryCredential ) {
                        dockerImage.push()
                    }
                }
            }
        }
    }
}